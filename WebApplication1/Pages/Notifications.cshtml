@page
@model WebApplication1.Pages.NotificationsModel
@{
}

<table class="table">
    <tr>
        <th>
            Title
        </th>
        <th>
            Message
        </th>
    </tr>



    @foreach (var notification in Model.Notifications)
    {
        <tr>
            <td>
                @notification.Title
            </td>
            <td>
                @notification.Body
            </td>
        </tr>
    }
</table>
